{
    "name": "spatie/laravel-link-checker",
    "description": "Check all links in a Laravel app",
    "keywords": [
        "spatie",
        "laravel-link-checker",
        "link",
        "checker",
        "crawler",
        "health"
    ],
    "homepage": "https://github.com/spatie/laravel-link-checker",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "spatie/crawler": "^1.1.1"

    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\LinkChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\LinkChecker\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
